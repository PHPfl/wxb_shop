
.container {
  font-size: 32rpx;
}
.title {
  padding: 100rpx 40rpx;
}
.title>view:first-child {
  margin-bottom: 20rpx;
}

.iptbox {
  padding: 0 40rpx 40rpx;
}
.iptbox input{
  border-bottom: 1px solid #eaeaea;
  padding: 15rpx 0;
  margin-top: 5rpx;
}
.mima {
  display: flex;
  justify-content: space-between;
  color: #2F72FF;
  margin-top: 20rpx;
}

.telbox {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #eaeaea;
}
.telbox >input {
  flex: 1;
  padding-right: 30rpx;
  border-bottom: none;
  
}



.btnbox {
  text-align: center;
  position: absolute;
  bottom: 100rpx;
  width: 100%;
}

.btn {
  height: 100rpx;
  width: 670rpx;
  margin: 0 auto 30rpx;
  color: #fff;
  border-radius: 20rpx;
  text-align: center;
  line-height: 100rpx;
  box-shadow: 10rpx 10rpx 10rpx #CBDAFA;
}

