/* pages/wytg/wytg.wxss */

.container {
  background: #f5f5f5;
  padding-top: 30rpx;
}
.cardbox {
  background: #2F72FF;
  width: 700rpx;
  margin: 0 auto 0;
  color: #fff;
  border-radius: 20rpx;
  padding: 30rpx ;
}
.imgbox {
  width: 640rpx;
  height: 600rpx;
  padding: 20rpx;
  background: #fff;
  margin: 0 auto 30rpx;
}
.line {
  border-top: 1px solid #fff;
  margin: 30rpx 0;
}
.logobox {
  display: flex;
}
.logo {
  font-size: 60rpx;
  padding-top: 20rpx
}
.lgcenter {
  flex: 1;
  padding-right: 20rpx;
  text-align: right;
  font-size: 24rpx;
  padding-top: 26rpx;
}
.ewm {
  width: 120rpx;
  height: 120rpx
}
.canvas {
  position: absolute;
  z-index: -99;
  top: 0;
}
.btnbox {
  display: flex;
  margin: 50rpx 30rpx 0 30rpx;
  justify-content: space-between;
}
.btnbox > view {
  width: 48%;
  text-align: center;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  border-radius: 20rpx;
  font-size: 38rpx;
}
.btnbox > view:last-child {
  background: #2F72FF;
  color: #fff;
}



.model {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0,0,0,.0);
  z-index: 99;
}
.popcontent {
  position: absolute;
  z-index: 100;
  background: #fff;
  left: 0;
  right: 0;
  bottom: 0;
  height: 200rpx;
  padding: 30rpx;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;

  display: flex;
  justify-content: space-between;
}
.popitem {
  width: 20%;
  text-align: center;
}
.popimg {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin:0 auto 20rpx;
}